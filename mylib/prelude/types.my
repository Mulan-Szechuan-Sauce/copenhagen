type Functor:
  def map(self, function)

type Foldable:
  def foldl(self, initial, folder)
  def len(self)

type Applicative:
  def apply(self, input)

type Monad:
  def bind(self, function)
  def wrap(value)

type Equal:
  def equals(self, self)

type Ordered:
  def compare(self, self)

class Ordering:
  LT
  GT
  EQ

type Show:
  def str(self)