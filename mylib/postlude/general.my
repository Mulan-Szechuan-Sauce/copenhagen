def id(x):
  return x

def const(value):
  return (_): value

def flip(f):
  return (a, b): f(b, a)

def uncurry(f):
  return ((a, b)): f(a, b)

def until(initial, predicate, func):
  return if predicate(initial):
    initial
  else:
    until(func(initial), predicate, func)

def comp(first, second):
  return (a): first(second(a))